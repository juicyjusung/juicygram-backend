{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import"
  ],
  "rules": {
    "no-unused-vars": 1,
    "comma-dangle": 1,
    "import/newline-after-import": 1,
    "no-use-before-define": 1,
    "no-console": [
      "warn",
      {
        "allow": [
          "error"
        ]
      }
    ]
  }
}
